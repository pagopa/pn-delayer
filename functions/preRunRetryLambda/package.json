{
  "name": "preRunRetryLambda",
  "version": "1.0.0",
  "description": "Pre-run retry algorithm Lambda",
  "main": "src/app/eventHandler.js",
  "scripts": {
    "test": "nyc mocha './src/test/**/*.test.js' --recursive --timeout=5000 --exit -r dotenv/config",
    "coverage": "nyc report --reporter=lcov",
    "sonar": "node sonarqube-scanner.js -Dsonar.login=${SONAR_TOKEN}",
    "build": "npm prune --production && rm -f function.zip && zip -r function.zip . -x './src/test/*' -x '*.md' -x '*.env' -x './coverage/*' -x './.nyc_output/*' -x './.scannerwork/*'",
    "test-build": "npm run test && npm run coverage && npm run sonar && npm run build"
  },
  "dependencies": {
    "@aws-sdk/client-sfn": "^3.540.0",
    "@aws-sdk/client-scheduler": "^3.540.0",
    "@js-joda/core": "^5.6.5",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "aws-sdk-client-mock": "^2.0.1",
    "chai": "^4.3.6",
    "eslint": "^8.23.1",
    "eslint-plugin-import": "^2.26.0",
    "lambda-tester": "^4.0.1",
    "mocha": "^9.2.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^18.0.0",
    "sonarqube-scanner": "^3.3.0"
  },
  "author": "",
  "license": "ISC"
}
